<div class='user-id-form'>
  <div>
    <h1>Flickr Foto Finder</h1>
    <%= form_with(url: "/", method: "get", local: true) do |form| %>
      <div>
        <%= form.text_field(:user_id, placeholder: "Enter Flickr ID")%>
      </div>
      <div>
        <%= form.submit "Enter" %>
      </div>
    <% end %>
  </div>
</div>

