<h1><%= "#{@user.username}'s Events"%></h1>

<%= link_to 'New Event', new_event_path %>

<% @user.created_events.each do |event| %>
  <div class='event-listing'>
    <h2><%= event.title %></h2><br>
    <p><%= event.description %></p>
    <%= link_to 'Go to Event', event_path(event.id) %>
  </div>
<% end %>